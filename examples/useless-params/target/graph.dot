digraph {
    node [fontname="bold helvetica"];
    edge [fontname="bold helvetica"];
    n0 [label="n0: <unfold>"];
    n1 [label="n1: <fold>"] [style=filled];
    n0 -> n1;
    n1 -> n0 [style=dashed];
    n2 [label="n2: x"];
    n1 -> n2 [label=" _y "];
    n3 [label="n3: x"];
    n1 -> n3 [label=" _z "];
    n4 [label="n4: x"];
    n1 -> n4 [label=" x "];
}
